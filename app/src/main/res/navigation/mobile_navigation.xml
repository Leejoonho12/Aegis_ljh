<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_main">

    <fragment
        android:id="@+id/nav_main"
        android:name="com.example.myapplication1213.ui.main.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_worker"
        android:name="com.example.myapplication1213.ui.workers.WorkersFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_workers" />

    <fragment
        android:id="@+id/nav_sensor"
        android:name="com.example.myapplication1213.ui.sensor.SensorFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_sensor" />

    <fragment
        android:id="@+id/nav_workerSearch"
        android:name="com.example.myapplication1213.ui.workerSearch.WorkerSearchFragment"
